            margin-bottom: 30px;
        }
        
        .category-nav ul {
            display: flex;
            flex-wrap: wrap;
            list-style: none;
            gap: 10px;
        }
        
        .category-nav ul li a {
            display: block;
            padding: 8px 16px;
            background-color: #fff;
            border-radius: 20px;
            color: #555;
            text-decoration: none;
            font-weight: 500;
            transition: all 0.3s;
            border: 1px solid #dee2e6;
        }
        
        .category-nav ul li a:hover, 
        .category-nav ul li a.active {
            background-color: #007bff;
            color: #fff;
            border-color: #007bff;
        }
        
