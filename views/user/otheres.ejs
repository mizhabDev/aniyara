            text-decoration: none;
            color: #555;
            font-weight: 500;
            transition: color 0.3s;
        }
        
        nav ul li a:hover {
            color: #007bff;
        }
        
        .header-icons {
            display: flex;
            align-items: center;
        }
        
        .icon-button {
            background: none;
            border: none;
